{
  "version": 2,
  "builds": [
    {
      "src": "loric_genealogy/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.13",
        "buildCommand": "bash build_files.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/loric_genealogy/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}