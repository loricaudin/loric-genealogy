{
  "version": 2,
  "builds": [
    {
      "src": "loric_genealogy/wsgi.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.13" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "static" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "loric_genealogy/wsgi.py"
    }
  ]
}